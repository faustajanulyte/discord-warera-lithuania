{
  "name": "discord-warera-lithuania",
  "version": "2.0.0",
  "description": "Discord bot for managing rooms and roles - Traditional Gateway Bot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "register-commands": "tsx src/scripts/register-commands.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}